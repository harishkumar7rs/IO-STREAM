//Reader
package com.iostream;

import java.io.*;

public class Main {

    public static void main(String[] args) throws Exception{
        try {
            char c[] ={'a','b','c','d','e','f'};
            CharArrayReader cr =new CharArrayReader(c);
            int x;
            while ((x= cr.read())!=-1){
                System.out.print((char)x);
            }
            cr.close();

        }catch (Exception e){
            System.out.println(e);
        }
    }
}
//Writer
